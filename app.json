{
  "expo": {
    "name": "weather-forecast",
    "slug": "weather-forecast",
    "version": "1.0.0",
    "assetBundlePatterns": ["**/*"],
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permitir que $(PRODUCT_NAME) use a sua localização."
        }
      ]
    ],
    "android": {
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "package": "com.viniciusamaral.weatherforecast"
    },
    "ios": {
      "bundleIdentifier": "com.viniciusamaral.weatherforecast"
    }
  }
}
